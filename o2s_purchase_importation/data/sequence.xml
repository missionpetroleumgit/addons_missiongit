			<field name="name">Importation Requisition Procura</field>
			<field name="code">importation.requisition.procura</field>
			<field name="prefix">MP-REQ-IMP-PROC-</field>
			<field name="padding">7</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
