                    </group>
                </search>
            </field>
        </record>

        <record id="form_importation_procura_order_inherit" model="ir.ui.view">
            <field name="name">form.importation.procura.order.inherit</field>
            <field name="model">importation.order</field>
            <field name="arch" type="xml">
                <form string="Orden de Importación">
                    <header>
                        <button name="rqimportation_sent" type="object" string="Enviar Aprobacion"
                                attrs="{'invisible':['|', ('state','!=','draft'),('imp_sent','!=', False)]}"
                        /><!--groups="o2s_purchase_importation.group_procura_importation_creation"-->
                        <button name="approve_imp_procura" string="Aprobar Orden"
                                groups="purchase_tiw.group_purchase_manager_approve_procura"
                                type="object" class="oe_highlight" states="draft"/>
                        <button name="purchase_confirm" type="object" string="Confirm Order" class="oe_highlight"
                                attrs="{'invisible':['|', ('imp_sent','=', False), ('state','in', ('draft', 'done'))]}"

                                confirm="Esta seguro de confirmar la Importacion ?"/>
                        <button name="imp_cancel" string="Cancelar"
                                type="object" states="draft,execute,done" confirm="Esta seguro que quiere cancelar la orden ?"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,execute,done"/>
                    </header>
                    <sheet>
                        <group col="4" colspan="2">
                            <field name="name" attrs="{'readonly':[('state', 'in', ['done', 'cancel'])]}"/>
                            <field name="partner_id" attrs="{'readonly':[('state', 'in', ['done', 'cancel'])]}"/>
                            <field name="company_id" attrs="{'readonly':[('state', 'in', ['done', 'cancel'])]}"/>
                            <field name="employee_id" readonly="1"/>
                            <field name="user_id" invisible="1"/>
                            <field name="date_order" required="1" attrs="{'readonly':[('state', 'in', ['done', 'cancel'])]}"/>
                            <field name="imp_origin" required="1"/>
                            <field name="with_copy"/>
                            <field name="attn"/>
                            <field name="register_id"/>
                            <field name="currency_id" groups="base.group_multi_currency"
                                   attrs="{'readonly':[('state', 'in', ['done', 'cancel'])]}"/>
                            <field name="imp_sent" invisible="1"/>
                            <field name="is_imp_procura" invisible="1"/>
                        </group>
                        <group>
                            <label for="importation_req_number"/>
                            <strong><field name="importation_req_number" style="color:darkblue !important"/></strong>
                        </group>
                        <group>
                            <strong><field name="importation_state" style="color:silver !important;font-size:16px"/></strong>
                        </group>
                        <notebook>
                            <page string="Solicitud de Producto"
                                  attrs="{'invisible':[('state','not in', ['draft','execute'])]}">
                                <field name="request_ids" attrs="{'invisible':[('state','!=', 'draft')]}">
                                    <tree editable="bottom">
                                        <field name="product_id"/>
                                        <field name="description"/>
                                        <field name="analytics_id" required="1"/>
                                        <field name="qty"/>
                                        <field name="qty_available"/>
                                        <field name="virtual_available"/>
                                        <field name="product_uom"/>
                                        <field name="date_requested"/>
                                        <field name="importation_id" invisible="1"/>
                                    </tree>
                                </field>
                                <field name="id" invisible="1"/>
                                <field name="quotes" attrs="{'invisible':['|', ('state','not in', ['draft','execute']), ('id','=', False)]}" context="{'importation_id':id}"/>
                            </page>
                            <page string="Líneas de Pedido" attrs="{'readonly':[('state', '=', 'approved')], 'invisible':[('state', '!=', 'done')]}">
                                <field name="order_lines" attrs="{'readonly':[('state', '=', 'approved')], 'invisible':[('state', '!=', 'done')]}">
                                    <tree string="Purchase Order Lines" editable="bottom">
                                        <field name="product_id"/>
                                        <field name="number"/>
                                        <field name="name"/>
                                        <field name="date_planned"/>
                                        <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}"/>
                                        <field name="product_qty"/>
                                        <field name="product_uom"/>
                                        <field name="price_unit"/>
                                        <field name="taxes_id" widget="many2many_tags" domain="[('parent_id','=',False),('type_tax_use','!=','sale')]"/>
                                        <field name="subtotal_importation"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                        <group class="oe_subtotal_footer oe_right">
                            <field name="amount_total" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                        </group>
                        <group>
                            <field name="payment_terms_id"/>
                            <field name="imp_delivery_date"/>
                            <field name="shipment_terms"/>
                        </group>
                        <div class="oe_clear">
                            <label for="comment"/>
                        </div>
                        <field name="comment"/>
                    </sheet>
                </form>
            </field>
        </record>

