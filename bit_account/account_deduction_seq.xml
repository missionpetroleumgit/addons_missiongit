<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--
        Account Deduction
        -->

        <record id="sequence_payable_deduction_t" model="ir.sequence.type">
            <field name="name">Account payable deduction</field>
            <field name="code">account.payable.deduction</field>
        </record>
        <record id="sequence_payable_deduction" model="ir.sequence">
            <field name="name">Account payable deduction</field>
            <field eval="5" name="padding"/>
            <field name="prefix">SUPPLIER/%(year)s/</field>
            <field name="code">account.payable.deduction</field>
        </record>
        
      	<record id="sequence_receivable_deduction_t" model="ir.sequence.type">
            <field name="name">Account receivable deduction</field>
            <field name="code">account.receivable.deduction</field>
        </record>
        <record id="sequence_receivable_deduction" model="ir.sequence">
            <field name="name">Account receivable deduction</field>
            <field eval="5" name="padding"/>
            <field name="prefix">CUSTOMER/%(year)s/</field>
            <field name="code">account.receivable.deduction</field>
        </record>
        
    </data>
</openerp>
