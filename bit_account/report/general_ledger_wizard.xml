<?xml version="1.0"?>
<openerp>
    <data>
        <record id="account_general_ledger_extend" model="ir.ui.view">
            <field name="name">General Ledger Extend</field>
            <field name="model">account.report.general.ledger</field>
            <field name="inherit_id" ref="account.account_common_report_view"/>
            <field name="arch" type="xml">
                <button string="Cancel" position="attributes">
                    <attribute name="invisible">1</attribute>
                </button>
                <button name="check_report" position="replace">
                    <button name="check_report" string="Print" type="object" default_focus="1" class="oe_highlight" attrs="{'invisible':[('id','==',False)]}"/>
                </button>
                <field name="chart_account_id" position="after">
                    <field name="specific_account_id"/>
                </field>
                <field name="filter" position="after">
                    <field name="type_export"/>
                    <field name="id" invisible="1"/>
                </field>
            </field>
        </record>

        <record id="account.action_account_general_ledger_menu" model="ir.actions.act_window">
            <field name="name">General Ledger</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.report.general.ledger</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="account.account_report_general_ledger_view"/>
            <field name="target">current</field>
        </record>
    </data>
</openerp>