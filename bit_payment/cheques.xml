<?xml version="1.0" encoding="utf-8"?>
<!-- vim:fdn=3:
-->
<openerp>
    <data>


        <record model="ir.actions.act_window" id="action_cheques">
			<field name="name">Redes</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">account.reporte_cheques</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
		</record>

		<record model="ir.ui.view" id="cheques_form_view">
			<field name="name">cheques_form_view</field>
			<field name="model">account.reporte_cheques</field>
			<field name="arch" type="xml">
                <form string="Red">
                    <sheet>
                        <header>
                            <button name="generar" type="object" class="object" string="Generar">Generar</button>
                        </header>
                        <group col="4">
                        <field name="company_id" groups="base.group_multi_company"/>
                        <field name="datefrom"/>
                        <field name="dateunto"/>
                        <field name="journal_id" domain="[('company_id', '=', company_id)], [('type','in',['bank', 'cash']),('is_paymenttpv','=',False)]"/>
                        </group>
                        <field name="partner_ids"/>
                    </sheet>
                </form>
            </field>
        </record>


        <menuitem id="menu_action_reporte_cheque" name="Reporte de Cheques" sequence="61"
          parent="account.menu_finance_bank_and_cash"  action="action_cheques"
          />


    </data>
</openerp>
