<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="report_print_egreso_hr">
		    <t t-call="report.html_container">
		        <t t-foreach="docs" t-as="o">
		            <div class="page">
			       <div class="row mt32 mb32">
                                  <div class="col-xs-4" style="position:absolute;left:510;top:20px;font-size: 20px">
                                    		    <t t-if="o.check">
					                <address t-field="o.benef"/>
					            </t>
						    <t t-if="not o.check">
					                <t t-if="o.type == 'payment'">
					                    <address t-field="o.partner_id.name"/>
					                </t>
					                <t t-if="o.type == 'advance'">
					                    <address t-field="o.partner_id.name"/>
					                </t>
					            </t>
                            </div>
                            <div class="col-xs-3" style="position:absolute;left:1290;top:22px;font-size: 19px"><h3>
                                    ***<span t-esc="o.to_string(o.amount)"/></h3><br/>
                            </div>
			            	</div>
			            	<div class="row mt32 mb32">
			            		<div class="col-xs-12" style="position:absolute;left:510;top:65px;font-size: 16px">
			            			<h3><span t-esc="get_amount_in_letters(o.amount)"/></h3>
			            		</div>
							</div>
		            		<div class="row mt32 mb32">
				        		<div class="col-xs-12" style="position:absolute;left:405;top:151px;font-size: 16px">
