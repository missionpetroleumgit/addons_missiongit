<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="report_account_nota">
    <t t-call="report.html_container">
    <t t-foreach="docs" t-as="o">
        <div class="page">
                <div style="position:absolute;left:700;top:140px"> 
                <p><span t-field="o.partner_id.phone"/></p>
                 <p><span t-field="o.partner_id.part_number"/></p>
                  <p><span/>.</p>
                </div>
            <div style="position:absolute;left:107;top:140px">
                  <p><span t-field="o.date_invoice"/></p>
		          <p><span t-field="o.partner_id.name"/>.</p>
		          <p><span t-field="o.partner_id.street"/>.</p>
            </div>
            <div style="position:absolute;left:50;top:250px;width:90%;">
            <table class="moz-email-headers-table" border="0" cellpadding="0" cellspacing="0">
                <thead>
                </thead>
                <tbody>
                    <tr t-foreach="o.invoice_line" t-as="l">
                        <td width="15%"><span t-field="l.quantity"/></td> 
                        <td width="70%"><span t-field="l.name"/></td>                        
                        <td width="13%"><span t-field="l.price_unit"/></td>
                        <td width="11%"><span t-field="l.price_subtotal"/></td>
                    </tr>
                </tbody>
            </table>
            </div>
            <div style="position:absolute;top:395px;left:790;">
                    <table class="table table-condensed">
                        <tr>
                            <td class="text-right">
                                <span t-field="o.amount_untaxed" t-field-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: &quot;o.currency_id&quot;}"/>
                            </td>
                            <td>
                            </td>
                        </tr>
                        
                        
                        <tr>
                            <td class="text-right">
<!--                                 <span t-field="o.get_invoice()" t-field-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: &quot;o.currency_id&quot;}"/> -->
                                      <span t-esc="o.get_invoice()" />
                             
                            </td>
                            <td>
                            </td>
                        </tr>
                        
                        
                        <tr>
                            <td class="text-right">
                            <span t-esc="0.00" />
                            </td>
                            <td>
                            </td>
                        </tr>
                        <tr>
                            <td class="text-right">
                                 <span t-field="o.amount_iva" t-field-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: &quot;o.currency_id&quot;}"/>
                            </td>
                            <td>
                            </td>
                        </tr>
                        
                        <tr>
                            <td class="text-right">
                                 <span t-field="o.amount_total" t-field-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: &quot;o.currency_id&quot;}"/>
                            </td>
                            <td>
                            </td>
                        </tr>
                        
                    </table>
            </div>
      </div>   
    </t>
</t>
</template>
