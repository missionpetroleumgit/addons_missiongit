<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="report_account_view_lpair">
    <t t-call="report.html_container">
    <t t-foreach="docs" t-as="o">
        <div class="page">
                <div style="position:absolute;left:700;top:119px">
                 <p><span t-field="o.date_invoice"/></p>
                 <p><span t-field="o.partner_id.part_number"/></p>
                 <p><span t-field="o.partner_id.phone"/></p>
                </div>

            <div style="position:absolute;left:117;top:119px">
                  <p><span/>.</p>
		          <p><span t-field="o.partner_id.name"/>.</p>
		          <p><span t-field="o.partner_id.street"/>.</p>
            </div>
            <div style="position:absolute;left:30;top:255px;width:90%;">
            <table class="moz-email-headers-table" border="0" cellpadding="0" cellspacing="0">
                <thead>
                </thead>
                <tbody>
                    <tr t-foreach="o.invoice_line" t-as="l">
                        <td width="15%"/>
                        <td width="98%"><span t-field="l.product_id.name"/></td>                        
                        <td width="60%"/>
                        <td class="text-right" width="6%"><span t-esc="round(l.price_subtotal,2)"/></td>
                    </tr>
                </tbody>
            </table>
            </div>
            <div style="position:absolute;top:407px;left:750;">
                    <table class="moz-email-headers-table" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td class="text-left"><strong><span t-esc="round(o.amount_untaxed,2)"/></strong></td>
                        </tr>
                        <tr>
	                    <td class="text-left"><strong><span t-esc="round(o.amount_iva,2)"/></strong></td>
                        </tr>
                        <tr>
                            <td class="text-left"><strong><span t-esc="round(o.amount_total_iva,2)"/></strong></td>
                        </tr>
                    </table>
            </div>
      </div>   
    </t>
</t>
</template>
