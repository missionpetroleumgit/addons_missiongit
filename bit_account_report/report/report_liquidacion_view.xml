<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="report_liquidacion_view">
    <t t-call="report.html_container">
    <t t-foreach="docs" t-as="o">
        <div class="page">
                <div style="position:absolute;left:700;top:157px"> 
                 <p><span t-field="o.partner_id.part_number"/></p>
                 <p><span t-field="o.partner_id.phone"/></p>
                </div>
            <div style="position:absolute;left:107;top:163px">
                  <p><span t-field="o.date_invoice"/></p>
		          <p><span t-field="o.partner_id.name"/>.</p>
		          <p><span t-field="o.partner_id.street"/>.</p>
            </div>
            <div style="position:absolute;left:50;top:270px;width:90%;">
            <table class="moz-email-headers-table" border="0" cellpadding="0" cellspacing="0">
                <thead>
                </thead>
                <tbody>
                    <tr t-foreach="o.invoice_line" t-as="l">
                        <td width="15%"><span t-field="l.quantity"/></td> 
                        <td width="70%"><span t-field="l.name"/></td>                        
                        <td width="16%"><span t-field="l.price_unit"/></td>
                        <td width="16%"><span t-esc="round(l.price_subtotal,2)"/></td>
                    </tr>
                </tbody>
            </table>
            </div>
            <div style="position:absolute;top:480px;left:810;">
                    <table class="moz-email-headers-table" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td class="text-left"><span t-esc="round(o.amount_untaxed,2)"/></td>
                        </tr>
                        <tr>
	                    <td class="text-left"><span t-esc="round(o.amount_iva,2)"/></td>
                        </tr>
                        <tr>
                            <td class="text-left"><span t-esc="round(o.amount_total_iva,2)"/></td>
                        </tr>
                    </table>
            </div>
      </div>   
    </t>
</t>
</template>
